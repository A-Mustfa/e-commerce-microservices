<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="001" author="ahmed">
        <insert tableName="item">
            <column name="name" value="pen"></column>
            <column name="description" value="writing items"></column>
            <column name="price" value="70"></column>
            <column name="stock" value="50"></column>
        </insert>
    </changeSet>
    <changeSet id="003" author="ahmed">
        <insert tableName="item">
            <column name="name" value="notebook"/>
            <column name="description" value="paper notebook for writing"/>
            <column name="price" value="120"/>
            <column name="stock" value="100"/>
        </insert>

        <insert tableName="item">
            <column name="name" value="pencil"/>
            <column name="description" value="graphite pencil"/>
            <column name="price" value="10"/>
            <column name="stock" value="300"/>
        </insert>

        <insert tableName="item">
            <column name="name" value="eraser"/>
            <column name="description" value="rubber eraser"/>
            <column name="price" value="5"/>
            <column name="stock" value="200"/>
        </insert>

        <insert tableName="item">
            <column name="name" value="marker"/>
            <column name="description" value="permanent marker"/>
            <column name="price" value="35"/>
            <column name="stock" value="80"/>
        </insert>

        <insert tableName="item">
            <column name="name" value="stapler"/>
            <column name="description" value="office stapler"/>
            <column name="price" value="150"/>
            <column name="stock" value="40"/>
        </insert>
    </changeSet>

</databaseChangeLog>